{
  "app.hideMenubarIcon": true,
  "app.launchOnStartup": true,
  "app.preferredTerminal": "iTerm2",
  "autocomplete.ai.enabled": true,
  "autocomplete.firstTokenCompletion": true,
  "autocomplete.fontFamily": "ShureTechMono Nerd Font",
  "autocomplete.fontSize": 17,
  "autocomplete.fuzzySearch": true,
  "autocomplete.height": 240,
  "autocomplete.hidePreviewWindow": false,
  "autocomplete.immediatelyExecuteAfterSpace": false,
  "autocomplete.insertSpaceAutomatically": true,
  "autocomplete.keybindings.command+-": "ignore",
  "autocomplete.keybindings.command+=": "ignore",
  "autocomplete.keybindings.command+i": "ignore",
  "autocomplete.keybindings.control+j": "ignore",
  "autocomplete.keybindings.control+k": "ignore",
  "autocomplete.keybindings.control+n": "ignore",
  "autocomplete.keybindings.control+p": "ignore",
  "autocomplete.keybindings.control+r": "ignore",
  "autocomplete.keybindings.down": "navigateDown",
  "autocomplete.keybindings.enter": "execute",
  "autocomplete.keybindings.shift+tab": "ignore",
  "autocomplete.keybindings.tab": "insertCommonPrefixOrInsertSelected",
  "autocomplete.keybindings.up": "navigateUp",
  "autocomplete.scrollWrapAround": true,
  "autocomplete.theme": "solarized-dark",
  "dotfiles.syncImmediately": false,
  "integrations.terminal.disabled": false
}
